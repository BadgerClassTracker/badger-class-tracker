{
  "name": "badger-class-tracker",
  "version": "1.0.0",
  "description": "UW Class enrollment tracker with email notifications",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "backend:build": "npm run build --workspace=backend",
    "backend:deploy": "npm run deploy --workspace=backend",
    "backend:dev": "npm run watch --workspace=backend",
    "frontend:build": "npm run build --workspace=frontend",
    "frontend:dev": "npm run dev --workspace=frontend",
    "frontend:start": "npm run start --workspace=frontend",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules backend/cdk.out"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "rimraf": "^5.0.0",
    "@babel/plugin-proposal-class-properties": "@babel/plugin-transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread": "@babel/plugin-transform-object-rest-spread",
    "node-domexception": false,
    "babel-runtime": {
      "core-js": "^2.6.12"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/badger-class-tracker"
  },
  "dependencies": {
    "core-js": "^2.6.12"
  }
}
